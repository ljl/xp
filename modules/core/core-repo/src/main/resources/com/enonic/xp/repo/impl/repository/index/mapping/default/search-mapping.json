{
  "_default_": {
    "_analyzer": {
      "path": "_document_analyzer"
    },
    "_all": {
      "enabled": false
    },
    "_source": {
      "enabled": true
    },
    "date_detection": false,
    "numeric_detection": false,
    "_timestamp": {
      "enabled": true
    },
    "properties": {
      "displayname._ngram": {
        "type": "string",
        "store": "no",
        "index": "analyzed",
        "index_analyzer": "ngram_index",
        "include_in_all": false
      }
    },
    "dynamic_templates": [
      {
        "template_fulltext": {
          "match": "*._analyzed",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "include_in_all": false
          }
        }
      },
      {
        "template_path": {
          "match": "*._path",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "index_analyzer": "path_analyzer",
            "search_analyzer": "path_analyzer",
            "include_in_all": false
          }
        }
      },
      {
        "template_ngram": {
          "match": "*._ngram",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "index_analyzer": "ngram_index_front",
            "include_in_all": false
          }
        }
      },
      {
        "template_userdata_date": {
          "match": "*._datetime",
          "mapping": {
            "type": "date",
            "store": "no",
            "index": "not_analyzed",
            "include_in_all": false
          }
        }
      },
      {
        "template_userdata_number": {
          "match": "*._number",
          "mapping": {
            "type": "double",
            "store": "no",
            "index": "not_analyzed",
            "include_in_all": false
          }
        }
      },
      {
        "template_userdata_orderby": {
          "match": "*._orderby",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "analyzer": "keywordlowercase",
            "ignore_above": 3072,
            "include_in_all": false
          }
        }
      },
      {
        "template_userdata_geopoint": {
          "match": "*._geopoint",
          "mapping": {
            "type": "geo_point",
            "store": "no",
            "index": "not_analyzed",
            "include_in_all": false
          }
        }
      },
      {
        "template_node_properties": {
          "match": "_*",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "analyzer": "keywordlowercase",
            "ignore_above": 3072,
            "include_in_all": false
          }
        }
      },
      {
        "template_metadata_strings": {
          "match": "*",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "analyzer": "keywordlowercase",
            "ignore_above": 3072,
            "include_in_all": false
          }
        }
      }
    ]
  }
}
